<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.ncl.di.MainWindowController">
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" hgap="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button cancelButton="true" mnemonicParsing="false" text="⨯ Cancel" />
            <Button mnemonicParsing="false" text="← Previous" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#next" text="Next →" fx:id="nextButton" disable="true"/>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </bottom>
</BorderPane>
